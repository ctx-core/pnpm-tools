{
	"name": "@ctx-core/pnpm-tools",
	"version": "9.2.155",
	"description": "Tools for the @ctx-core monorepo",
	"keywords": [
		"ctx-core"
	],
	"homepage": "https://github.com/ctx-core/pnpm-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pnpm-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pnpm-tools.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./project_json_",
		"./projects_json_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"projects_json_": "./bin/projects_json_.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^6.5.10",
		"dir-glob": "^3.0.1",
		"js-yaml": "4.1.0",
		"path-type": "^5.0.0",
		"tiny-glob": "^0.2.9"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}