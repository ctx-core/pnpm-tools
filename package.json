{
	"name": "@ctx-core/pnpm-tools",
	"version": "9.0.80",
	"description": "Tools for the @ctx-core monorepo",
	"keywords": [
		"ctx-core"
	],
	"homepage": "https://github.com/ctx-core/pnpm-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pnpm-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pnpm-tools.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"projects_json_": "./bin/projects_json_.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/array": "26.8.14",
		"@ctx-core/function": "^21.6.1",
		"dir-glob": "^3.0.1",
		"js-yaml": "4.1.0",
		"path-type": "^5.0.0",
		"tiny-glob": "^0.2.9"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.60",
		"@swc/core": "^1.3.31",
		"@types/glob": "8.0.1",
		"@types/js-yaml": "^4.0.5",
		"rimraf": "^4.1.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"type": "module",
	"svelte": "./lib/index.js",
	"sideEffects": false
}