{
	"name": "@ctx-core/pnpm-tools",
	"version": "9.0.9",
	"description": "Tools for the @ctx-core monorepo",
	"keywords": [
		"ctx-core"
	],
	"homepage": "https://github.com/ctx-core/pnpm-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pnpm-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pnpm-tools/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./index.js",
	"module": "./index.js",
	"types": "./index.ts",
	"bin": {
		"projects_json_": "./bin/projects_json_.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/function": "workspace:^20.0.11",
		"esm": "^3.2.25",
		"globby": "^11.0.4",
		"js-yaml": "4.1.0"
	},
	"devDependencies": {
		"@types/glob": "7.1.4",
		"@types/js-yaml": "^4.0.2",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"type": "module"
}
