{
	"name": "@ctx-core/pnpm-tools",
	"version": "9.0.24",
	"description": "Tools for the @ctx-core monorepo",
	"keywords": [
		"ctx-core"
	],
	"homepage": "https://github.com/ctx-core/pnpm-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pnpm-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pnpm-tools/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./index.js",
	"module": "./index.js",
	"types": "./index.ts",
	"bin": {
		"projects_json_": "./bin/projects_json_.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/function": "workspace:^20.2.4",
		"globby": "^12.0.2",
		"js-yaml": "4.1.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.108",
		"@types/glob": "7.2.0",
		"@types/js-yaml": "^4.0.4",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"type": "module"
}