{
	"name": "@ctx-core/pnpm-tools",
	"version": "9.0.35",
	"description": "Tools for the @ctx-core monorepo",
	"keywords": [
		"ctx-core"
	],
	"homepage": "https://github.com/ctx-core/pnpm-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pnpm-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pnpm-tools.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"projects_json_": "./bin/projects_json_.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@"
	},
	"dependencies": {
		"@ctx-core/function": "workspace:^20.2.15",
		"globby": "^12.0.2",
		"js-yaml": "4.1.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/glob": "7.2.0",
		"@types/js-yaml": "^4.0.5",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"type": "module",
	"svelte": "./dist/index.js"
}
